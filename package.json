    "acorn"                        : "5.0.3",
    "remarkable"                   : "1.7.1",
    "highlight.js"                 : "9.12.0",
    "uglify-js"                    : "3.0.16",
    "source-map"                   : "0.5.6",
    "mime"                         : "1.3.6",
    "nodemailer2"                  : "2.7.1",
    "nodemailer-sendmail-transport": "1.0.2",
    "easyimage"                    : "2.1.0",
    "express-session"              : "1.15.3",
    "passport"                     : "0.3.2",
    "compressible"                 : "2.0.10",
    "es5-shim"                     : "4.5.9",
    "domutils"                     : "1.6.2",
    "htmlparser2"                  : "3.9.2",
    "css-select"                   : "1.3.0-rc0",
    "url-pattern"                  : "1.0.3",
    "diff"                         : "3.2.0",
    "piexifjs"                     : "1.0.3"
